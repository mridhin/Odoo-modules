<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="remove_dup_province" model="ir.cron">
        <field name="name">Philippine: Remove Duplicate Province data</field>
        <field name="model_id" ref="model_res_country_state"/>
        <field name="state">code</field>
        <field name="code">model.remove_dup_records()</field>
        <field name='interval_number'>5</field>
        <field name='interval_type'>minutes</field>
        <field name="numbercall">1</field>
        <field name="active" eval="True"/>
    </record>

    <record id="remove_number_special_character" model="ir.cron">
        <field name="name">Philippine: Remove Number and Special Character from City</field>
        <field name="model_id" ref="model_res_state_city"/>
        <field name="state">code</field>
        <field name="code">model.remove_number_special_character()</field>
        <field name='interval_number'>5</field>
        <field name='interval_type'>minutes</field>
        <field name="numbercall">1</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
