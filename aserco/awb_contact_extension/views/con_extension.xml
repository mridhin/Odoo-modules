<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
<!-- Added the active toggle in the res.partner.industry -->
<record id="res_partner_industry_view_tree_inherit" model="ir.ui.view">
		<field name="name">res.partner.industry.inherit</field>
		<field name="model">res.partner.industry</field>
		<field name="inherit_id" ref="base.res_partner_industry_view_tree" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='active']" position="replace">
				 <field name="active" widget="boolean_toggle" />

			</xpath>
		</field>
	</record>
    </data>
</odoo>
