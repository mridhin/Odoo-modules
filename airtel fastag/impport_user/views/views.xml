<odoo>
  <data>
      <record model="res.groups" id="group_import_user">
            <field name="name">Import User</field>
            <field name="category_id" ref="record_rule.module_category_team"/>
        </record>
       <record id="add_user_action_view" model="ir.ui.view">
            <field name="name">Import Users</field>
            <field name="model">import.user</field>
            <field name="arch" type="xml">
                <form string="User Details">
                    <sheet>
                        <group>
                            <group>
                              <field name="fastag_user" filename="filename"/>
                                <field name="filename" invisible="1"/>

                            </group>
                            <group>
                                <button name="import_user_line"
                                        string="Import User"
                                        context="{'default_active_id': active_id}"
                                        type="object" class="oe_highlight"/>
                            </group>
                        </group>
                        <group>
                            <field name="import_line_form" readonly="1">
                                <tree>
                                    <field name="lapu_no"/>
                                    <field name="circle_id"/>
                                    <field name="project_id"/>
                                    <field name="work_city"/>
                                    <field name="name"/>
                                    <field name="doj"/>
                                    <field name="office_add"/>
                                    <field name="designation"/>
                                    <field name="tl_name"/>
                                    <field name="cm_name"/>
                                    <field name="em_name"/>
                                    <field name="email"/>
                                </tree>
                            </field>
                        </group>
                    </sheet>
                    <footer>
                        <button name="import_user" string="Confirm" type="object" class="oe_highlight"/>
                    </footer>
                </form>
            </field>
        </record>
    <record model="ir.actions.act_window" id="add_user_aaction_view">
            <field name="name">Import Users</field>
            <field name="res_model">import.user</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="add_user_action_view"/>
            <field name="target">new</field>
        </record>

        <menuitem id="user_import_menu" name="Import Users" action="add_user_aaction_view"
        parent="base.menu_users" groups="group_import_user"/>
  </data>
</odoo>