        <!-- stock picking form inherit -->

        <record id="rs_inherit_stock_picking_form_view" model="ir.ui.view">
            <field name="name">stock.picking.form.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                                <xpath expr="//header" position="inside">
                                  <button name="product_user_circle" type="object"
                                          groups="rn_vehical_class.group_admin_access"
                                          string="create all report" class="oe_highlight"/>
                                </xpath>

                <xpath expr="//header" position="inside">
                    <button name="transfer_update" type="object"
                            string="create transfer report"
                            groups="rn_vehical_class.group_admin_access"
                            class="oe_highlight"/>
                </xpath>
                <xpath expr="//header" position="inside">
                    <button name="transfer_delete" type="object"
                            groups="rn_vehical_class.group_admin_access"
                            string="delete transfer"
                            class="oe_highlight"/>
                </xpath>
                <xpath expr="//header" position="inside">
                    <button name="all_transfer_delete" type="object"
                            groups="rn_vehical_class.group_admin_access"
                            string="delete all transfer"
                            class="oe_highlight"/>
                </xpath>
                <!--                <xpath expr="//header" position="inside">-->
                <!--                    <button name="report_update" type="object"-->
                <!--                            string="Report Update"-->
                <!--                            class="oe_highlight"/>-->
                <!--                </xpath>-->

                <xpath expr="//field[@name='picking_type_id']" position="after">
                    <field name="rs_user_id" readonly="1"/>
                    <field name="rs_filter_fastag"/>
                    <field name="sequence_fastag"
                           attrs="{'invisible':[('rs_filter_fastag', '!=', True)]}"/>
                    <field name="fastag_count" sum="Total of Fastags"/>
                    <field name="fastag_count1" invisible="1"/>
                    <field name="rs_assi_date" invisible="1"/>
                </xpath>
                <xpath expr="//notebook" position="before">
                    <label attrs="{'invisible':['|',('rs_filter_fastag','=',False),('sequence_fastag','=',True)]}"
                           for="user_order_id" string="Create transfer using requested order"
                           groups="record_rule.group_warhouse_manager"
                        />
                    <group groups="record_rule.group_warhouse_manager"
                         attrs="{'invisible':['|',('rs_filter_fastag','=',False),('sequence_fastag','=',True)]}">
                        <field name="user_order_id" placeholder="Select User"
                               style="width: 25%" attrs="{'invisible': [('rs_filter_fastag','=',False)]}"
                           options="{'no_create_edit': True, 'no_quick_create': True}"/>

                        <field name="user_order_designation" placeholder="User Designation"
                               style="width: 25%; "
                               attrs="{'invisible': [('rs_filter_fastag','=',False)]}"/>

                        <field name="select_order_id" placeholder="Select order id"
                               style="width: 25%;"
                               options="{'no_create_edit': True, 'no_quick_create': True}"
                           attrs="{'invisible': [('rs_filter_fastag','=',False)]}"/>
                        <field name="order_id_bool" invisible="1"/>
                    </group>
<!--                    <field name="user_order_id"-->
<!--                           attrs="{'invisible': [('rs_filter_fastag','=',False)]}"-->
<!--                           options="{'no_create_edit': True, 'no_quick_create': True}"/>-->
<!--                    <field name="user_order_designation" attrs="{'invisible': [('rs_filter_fastag','=',False)]}"/>-->
<!--                    <field name="select_order_id" options="{'no_create_edit': True, 'no_quick_create': True}"-->
<!--                           attrs="{'invisible': [('rs_filter_fastag','=',False)]}"/>-->
                </xpath>
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="rs_emp_no" invisible="1" readonly="0"/>
                    <field name="rs_destination_id" required="0" invisible="0"
                           readonly="1"/>
                    <field name="rs_office_addres"/>
                </xpath>
                <xpath expr="//field[@name='picking_type_id']"
                       position="attributes">
                    <attribute name="required">0</attribute>
                </xpath>
                <xpath expr="//field[@name='location_id']"
                       position="attributes">
                    <attribute name="required">0</attribute>
                </xpath>
                <xpath expr="//field[@name='location_dest_id']"
                       position="attributes">
                    <attribute name="required">0</attribute>
                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_id']"
                       position="after">


                    <field name="fatag_bracode"/>

                    <field name="circle_name"/>
                    <field name="date_of_dispatch"/>
                    <field name="consignment_no"/>
                    <field name="delivery_partner"/>
                    <field name="emp_mobile_no"/>
                    <field name="rs_faulty_stag"/>
                    <field name="fastag_sold"/>
                    <field name="unlink_fastag"/>
                    <field name="category_id"/>
                    <field name="check_move_line"/>
                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']/tree/
                field[@name='product_uom_qty']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']/tree/
                field[@name='product_id']"
                       position="replace">
                    <field name="product_id"
                           domain="[('fastag_sold','=','no'),('rs_faulty_stag','=','no'),('unlink_fastag','=',False)]"/>
                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']/tree/
                field[@name='forecast_availability']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']"
                       position="before">

                    <label attrs="{'invisible':[('rs_filter_fastag','=',False)]}"
                           for="no_fastags" string="Filter Fastags "/>
                    <div class="address_format"
                         attrs="{'invisible':[('rs_filter_fastag','=',False)]}">
                        <field name="no_fastags" placeholder="No OF Fastags"
                               style="width: 20%" force_save="1"
                               attrs="{'invisible':[('sequence_fastag','=',True)],
                               'readonly':[('select_order_id','!=',False)]}"/>

                        <field name="from_fastags" placeholder="From fastag"
                               style="width: 18%; margin-left:3px;"
                               attrs="{ 'invisible':[('sequence_fastag','=',False)]}"/>

                        <field name="to_fastags" placeholder="To fastag"
                               style="width: 18%;  margin-left:3px;"
                               attrs="{'invisible':[('sequence_fastag','=',False)]}"/>

                        <field name="rs_circle" placeholder="Select Circle"
                               options="{'no_create': True}"
                               style="width: 10%;margin-left:2%" force_save="1"
                               attrs="{'required': [('rs_filter_fastag','=',True)],
                               'readonly':[('select_order_id','!=',False)]}"/>

                        <field name="rs_emp_mob_no"
                               placeholder="Enter Mobile No"
                               style="width: 13%;margin-left:2%" force_save="1"
                               attrs="{'required': [('rs_filter_fastag','=',True)],
                               'readonly':[('select_order_id','!=',False)]}"/>

                        <field name="category_id"
                               placeholder="Enter Tag Class"
                               style="width: 15%;margin-left:2%" force_save="1"
                               attrs="{'readonly':[('select_order_id','!=',False)]}"/>

                        <button string="Submit" name="get_serial_wise_barcode"
                                type="object"
                                style="margin-left:25px;width: 13%;line-height: 1.2;margin-top: -0.5%;"
                                class="oe_highlight"/>
                    </div>

                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']/tree/
                field[@name='quantity_done']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']"
                       position="attributes">
                    <attribute name="mode">tree</attribute>
                </xpath>
                <xpath expr="//header" position="inside">
                    <button string="Upload Products"
                            name="%(add_product_line_action_view)d"
                            class="oe_highlight" type="action"
                            attrs="{'invisible':['|', ('rs_state','not in',['draft']),('rs_filter_fastag','=',True)]}"/>
                </xpath>
                <xpath expr="//field[@name='state']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//header" position="inside">
                    <field name="rs_state" widget="statusbar"
                           statusbar_visible=" "/>
                </xpath>
                <xpath expr="//form/sheet/notebook/page[@name='extra']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//form/sheet/notebook/page[@name='note']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='origin']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//label[@for='partner_id']" position="replace">
                </xpath>
                <xpath expr="//field[@name='picking_type_id']"
                       position="attributes">
                    <attribute name="required">1</attribute>
                </xpath>
                <xpath expr="//field[@name='partner_id']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='location_id']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='location_dest_id']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//form/sheet/notebook/page[@name='detailed_operations_shown']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>

        <!-- inherit tree view stock picking -->

        <record id="rs_inherit_stock_picking_tree_view" model="ir.ui.view">
            <field name="name">stock.picking.tree.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='state']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='origin']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='partner_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='company_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>


                <xpath expr="//field[@name='scheduled_date']" position="after">
                    <field name="rs_circle_id"/>
                    <field name="fastag_trnsfr_date" readonly="1"/>
                    <field name="rs_user_id"/>
                    <field name="rs_destination_id"/>
                    <field name="rs_state"/>
                    <field name="fastag_count" string="Assign Fastags"/>
                    <field name="fastag_sold" sum="Total of Sold"/>
                    <field name="fastag_faulty" sum="Total of Faulty"/>
                    <field name="fastag_count1" invisible="1"/>
                    <field name="select_order_id" />
                </xpath>


            </field>
        </record>

        <!-- inherit search view stock picking -->

        <record id="view_picking_internal_search_filter" model="ir.ui.view">
            <field name="name">stock.picking.internal.search.filter</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_internal_search"/>
            <field name="arch" type="xml">

                <xpath expr="//filter[@name='status']" position="after">
                    <filter string="Circle" name="circle_name" domain="[]"
                            context="{'group_by': 'rs_circle'}"/>
                    <filter string="Assign To" name="assign_to" domain="[]"
                            context="{'group_by': 'rs_destination_id'}"/>
                    <filter string="Status" name="all_state" domain="[]"
                            context="{'group_by': 'rs_state'}"/>

                </xpath>
                <!--hide status group filter -->

                <xpath expr="//filter[@name='status']" position="attributes">

                    <attribute name="invisible">1</attribute>

                </xpath>
                <xpath expr="//filter[@name='origin']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='picking_type']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//filter[@name='starred']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='waiting']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='done']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='late']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='planning_issues']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='backorder']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='activities_exception']"
                       position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//filter[@name='draft']" position="attributes">
                    <attribute name="string">Assign</attribute>
                </xpath>

                <xpath expr="//filter[@name='available']" position="attributes">
                    <attribute name="string">Acknowledge</attribute>
                </xpath>
                <xpath expr="//filter[@name='cancel']" position="attributes">
                    <attribute name="string">Cancel/Decline</attribute>
                </xpath>
                <xpath expr="//filter[@name='my_transfers']" position="after">
                    <separator/>
                    <filter string="Transfers" name="fastag_count"
                            domain="[('fastag_count1','!=',0)]"/>
                </xpath>
            </field>
        </record>

        <record id="view_hr_wizard" model="ir.ui.view">
            <field name="name">Warning</field>
            <field name="model">seequence.wizard</field>
            <field name="arch" type="xml">
                <form string="generation fichier">
                    <notebook>
                        <page string="Sold Brcodes"
                              attrs="{'invisible': [('sold', '=', False)]}">
                            <field name="sold"/>
                        </page>
                        <page string="Faulty Brcodes"
                              attrs="{'invisible': [('fault', '=', False)]}">
                            <field name="fault"/>
                        </page>
                        <page string="Unlink Brcodes"
                              attrs="{'invisible': [('unlink', '=', False)]}">
                            <field name="unlink"/>
                        </page>
                    </notebook>
                    <footer>
                        <button name="method" string="Continue"
                                confirm="Do you want to continue?" type="object"
                        />
                        <button string="Cancel" name="cancel" type="object"
                        />
                    </footer>
                </form>
            </field>
        </record>
        <record id="stock.action_picking_tree_all"
                model="ir.actions.act_window">
            <field name="name">Transfer</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock.picking</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id"
                   ref="view_picking_internal_search_filter"/>
            <field name="context">{"search_default_fastag_count":1}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No product found. Let's create one!
                </p>
            </field>
        </record>
