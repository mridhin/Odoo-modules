<odoo>
    <data>

        <record id="rn_vehical_class_view_form" model="ir.ui.view">
            <field name="name">vehical.class.form</field>
            <field name="model">vehical.class</field>
            <field name="arch" type="xml">
                <form string="Vehicle Class">
                    <header>
                        <field name="state" widget="statusbar"/>
                        <button name="action_confirm"
                                string="Confirm" type="object"
                                class="oe_highlight"
                                attrs="{'invisible':[('state','in',['confirm'])]}"
                        />
                    </header>
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group col="4" name="vehical_details"
                               string="Vehicle Details">
                            <field name="partner_id" required="1"/>
                            <field name="barcode"/>
                            <field name="tag_id"/>
                            <field name="inword_data"/>
                            <field name="rn_category_id" string="Vehicle Type"/>
                            <field name="category_id" widget="many2many_tags"
                                   options="{'color_field': 'color', 'no_create_edit': True}"
                                   placeholder="Tags..."/>
                            <field name="v_type_id"/>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids"
                               groups="base.group_user"/>
                        <field name="message_ids"/>
                    </div>
                </form>
            </field>
        </record>
        <!-- vehical tree view -->
        <record id="vehical_view_tree" model="ir.ui.view">
            <field name="name">vehical.tree</field>
            <field name="model">vehical.class</field>
            <field name="arch" type="xml">
                <tree string="Vehical Class">
                    <field name="name"/>
                    <field name="barcode"/>
                    <field name="rn_category_id" string="Vehicle Type"/>
                    <field name="inword_data"/>
                </tree>
            </field>
        </record>
        <record id="vehical_action_form" model="ir.actions.act_window">
            <field name="name">Vehicle Class</field>
            <field name="res_model">vehical.class</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <field name="view_id" ref="vehical_view_tree"/>
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                    Click to add a new record.
                </p>
                <p>
                    <!-- More details about what a user can do -->
                </p>
            </field>
        </record>

