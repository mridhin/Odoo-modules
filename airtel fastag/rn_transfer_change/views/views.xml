<!--FILTER FOR stock picking tree view BY NAME IN RECORDS-->
<record id="stock_picking_search_filters" model="ir.ui.view">
  <field name="name">stock.picking.view.search</field>
  <field name="model">stock.picking</field>
  <field name="inherit_id" ref="stock.view_picking_internal_search"/>
  <field name="arch" type="xml">
    
        <!-- <group>
          <filter name="rs_destination_id" string="Assign To " />
        </group> -->

