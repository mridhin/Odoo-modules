<odoo>
    <data>
        <!--        unlink mosel search view-->
        <record model="ir.ui.view" id="hotel_booking_search">
            <field name="name">Unlink Tag Search</field>
            <field name="model">unlinked_tag.unlinked_tag</field>
            <field name="arch" type="xml">
                <search>

                    <filter name="matched_unlink" string="Matched Unlink"
                            domain="[('matched', '!=', False),('unmatched', '=', False)]"/>
                    <filter name="mismatched_unlink" string="Mismatched Unlink"
                            domain="[('matched', '=', False),('unmatched', '!=', False)]"/>
                    <separator/>
<!--                    <group expand="1" string="Group By">-->
<!--                        <filter string="Matched Unlink"-->
<!--                                name='matched_unlink_group'-->
<!--                                context="{'group_by':'matched_unlink_record'}"/>-->
<!--                    </group>-->

                </search>
            </field>
        </record>

        <!--      unlink model tree view-->
        <record model="ir.ui.view" id="unlinked_tag_tree">
            <field name="name">Unlinked Tag</field>
            <field name="model">unlinked_tag.unlinked_tag</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <header>
                        <button name="update_unlink_circle"
                                string="Update Circle" type="object"
                                class="oe_highlight"
                        />
                    </header>
                    <field name="name"/>
                    <field name="month"/>
                    <field name="bar_code"/>
                    <field name="tag_id"/>
                    <field name="toll_plaza"/>
                    <field name="amount"/>
                    <field name="circle_id"/>
                    <field name="product_assign_to" invisible="1"/>
                    <field name="employee_id" invisible="1"/>
                    <field name="matched_unlink_record" invisible="1"/>
                </tree>
            </field>
        </record>

        <!--      unlink model form view-->
        <record model="ir.ui.view" id="unlinked_tag_from">
            <field name="name">unlinked</field>
            <field name="model">unlinked_tag.unlinked_tag</field>
            <field name="arch" type="xml">
                <form create="false" edit="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="bar_code"/>
                                <field name="name"/>
                                <field name="month"/>
                                <field name="tag_id"/>
                            </group>
                            <group>
                                <field name="toll_plaza"/>
                                <field name="amount"/>
                                <field name="circle_id"/>
                                <field name="flag"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!--      wizard form view-->
        <record model="ir.ui.view" id="check_wizard_form">
            <field name="name">Unlink Tag</field>
            <field name="model">check.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="unlink_product"
                                       filename="filename"/>
                                <field name="filename" invisible="1"/>
                            </group>
                            <group>
                                <button name="upload_file"
                                        string="Import Records"
                                        type="object" class="oe_highlight"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Sheets Records" invisible="1">
                                <field name="unlink_ids"
                                       readonly="1">
                                    <tree>
                                        <field name="name1"/>
                                        <field name="month"/>
                                        <field name="bar_code"/>
                                        <field name="tag_id"/>
                                        <field name="toll_plaza"/>
                                        <field name="amount"/>
                                        <field name="circle_id"/>
<!--                                        <field name="product_assign_to"/>-->
<!--                                        <field name="employee_id"/>-->
                                    </tree>
                                </field>
                            </page>
                            <page string="Matched Records">
                                <field name="unlinked_maching_ids"
                                       readonly="1">
                                    <tree>
                                        <field name="name1"/>
                                        <field name="month"/>
                                        <field name="bar_code"/>
                                        <field name="tag_id"/>
                                        <field name="toll_plaza"/>
                                        <field name="amount"/>
                                        <field name="circle_id"/>
                                        <field name="product_assign_to"/>
                                        <field name="employee_id"/>
<!--                                        <field name="flag"/>-->
                                    </tree>
                                </field>
                            </page>
                            <page string="Unmatched Records">
                                <field name="unlinked_mismaching_ids"
                                       readonly="1">
                                    <tree>
                                        <field name="name1"/>
                                        <field name="month"/>
                                        <!--                                        <field name="bar_code"/>-->
                                        <field name="tag_id"/>
                                        <field name="toll_plaza"/>
                                        <field name="amount"/>
                                        <!--                                        <field name="circle_id"/>-->
                                    </tree>
                                </field>
                            </page>
                            <page string="Duplicate Records"  attrs="{'invisible': [('duplicate_ids', '=', False)]}">
                                <field name="duplicate_ids"
                                       readonly="1">
                                    <tree>
                                        <field name="name1"/>
                                        <field name="month"/>
<!--                                        <field name="bar_code"/>-->
                                        <field name="tag_id"/>
                                        <field name="toll_plaza"/>
                                        <field name="amount"/>
<!--                                        <field name="circle_id"/>-->
<!--                                        <field name="product_assign_to"/>-->
<!--                                        <field name="employee_id"/>-->
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                    <footer>
                        <button string="Accept" type="object" name="accept"
                                class="oe_highlight"/>
                        <button string="Dont Accept" type="object"
                                name="dontaccept" class="btn btn-default"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--      wizard action-->
        <record model="ir.actions.act_window" id="action_import_unlinked_tag">
            <field name="name">Missing Records</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">check.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="check_wizard_form"/>
            <field name="target">new</field>
        </record>

        <record model="ir.actions.act_window" id="action_unlinked_tag">
            <field name="name">Unlinked Tag</field>
            <field name="res_model">unlinked_tag.unlinked_tag</field>
            <field name="view_mode">tree,kanban,form</field>
            <field name="context">{'search_default_matched_unlink':1}</field>
        </record>

        <!--unlink menus-->
        <menuitem name="Unlinked Tag" id="unlinked_tag_menu_1"
                  parent="stock.menu_stock_root" sequence="4"/>
        <menuitem name="Unlinked tag" id="unlinked_tag_menu_2"
                  parent="unlinked_tag.unlinked_tag_menu_1"
                  action="action_unlinked_tag" sequence="2"/>
        <menuitem name="Import Unlink" id="unlinked_tag_menu_3"
                  parent="unlinked_tag.unlinked_tag_menu_1"
                  action="action_import_unlinked_tag" sequence="1"/>

        -->
    </data>
</odoo>
