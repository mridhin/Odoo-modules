<odoo>
    <record id="report_Philippine_group_1" model="res.groups">
        <field name="name">Report Viewer</field>
        <field name="category_id" ref="awb_philippine_tax_app.module_category_Philippine_0"/>
        <field name="implied_ids" eval="[(4, ref('account.group_account_manager'))]"/>
    </record>
    <record id="acc_Philippine_grouop_2" model="res.groups">
        <field name="name">Tax Accountant</field>
        <field name="category_id" ref="awb_philippine_tax_app.module_category_Philippine_0"/>
        <field name="implied_ids" eval="[(4, ref('report_Philippine_group_1'))]"/>
    </record>
    <record id="admin_Philippine_group_3" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="awb_philippine_tax_app.module_category_Philippine_0"/>
        <field name="implied_ids" eval="[(4, ref('acc_Philippine_grouop_2'))]"/>
    </record>
    <record id="base.default_user" model="res.users">
        <field name="groups_id" eval="[(4,ref('awb_philippine_tax_app.admin_Philippine_group_3'))]"/>
    </record>
