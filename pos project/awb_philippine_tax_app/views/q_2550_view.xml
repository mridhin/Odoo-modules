<odoo>
  <data>
        <record id="q_2550_view_form" model="ir.ui.view">
            <field name="name">q.2550.form</field>
            <field name="model">q.2550</field>
            <field name="arch" type="xml">
                <form string="2550Q PDF Report">
                    <header>
                        <button name="action_report_2550q" type="object" string="Generate Report" class="oe_highlight" attrs="{'invisible': [('awb_state', '=', 'draft')]}"/>
                        <button name="action_resetdraft_2550q" type="object" string="Reset To Draft" class="oe_highlight" attrs="{'invisible': ['|',
                            ('awb_state', '=', 'draft'),
                            ('awb_entry_count','=',True)]}"/>
                        <button name="action_validate" type="object" string="Validate" class="oe_highlight" attrs="{'invisible': [('awb_state', '!=', 'draft')]}"/>
                        <button name="generate_journal_Entry" type="object" string="Generate Journal Entry" class="oe_highlight" invisible="1"/>
                        <!-- attrs="{'invisible': ['|',('awb_state', '=', 'draft'),('awb_entry_count','=',True)]}" -->
                        <field name="awb_state" statusbar_visible="draft,validated,amendment" widget="statusbar"/>
                    </header>
                    <sheet>
                        <field name="awb_entry_count" invisible="1"/>
                        <field name="awb_move_id" invisible="1"/>
                        <div class="oe_button_box" name="button_box">
                            <button name="action_view_journal_entry_2550q" type="object" class="oe_stat_button" icon="fa-pencil-square-o" attrs="{'invisible': [('awb_entry_count', '=', False)]}" string="Journal Entry">
                            </button>
                        </div>
                        <group>
                            <group>
                                <field name="awb_partner_id" options="{'no_create_edit':True,'no_create': True,'no_open': True}" attrs="{'readonly': [('awb_entry_count', '=',True)]}"/>
                                <field name="awb_company_id" required="True" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                <field name="awb_fiscal_year" domain="[('company_id', '=', awb_company_id)]" attrs="{'invisible': [('awb_company_id','=', False)],'required':[('awb_company_id','!=', False)],'readonly': [('awb_state', '!=', 'draft')]}" options="{'no_create_edit': True,'no_create': True, 'no_open': True}"/>
                                <field name="awb_fiscal_quarter" domain="['|', ('awb_type', '=', 'quarterly'),('fiscalyear_id_qua','=', awb_fiscal_year)]" attrs="{'invisible': ['|',('awb_company_id','=', False), ('awb_fiscal_year', '=', False)], 'required':['|',
                                    ('awb_company_id','!=',False),
                                    ('awb_fiscal_year','!=',False)],
                                    'readonly': [('awb_state', '!=', 'draft')]}" 
                                    options="{'no_create_edit': True,'no_create': True,'no_open':True}" required="1"/>
                                <field name="awb_month" invisible="1"/>
                                <field name="awb_year" invisible="1"/>
                                <field name="awb_amended_return" required="1" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                <field name="awb_no_of_Sheets" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                <div>
                                    <button name="action_generate_report_values" id="action_generate_report_values" string="Generate Report Values" class="btn-primary" type="object" attrs="{'invisible':[('awb_state', '!=', 'draft')]}"/>
                                </div>
                            </group>
                            <group>
                                <field name="awb_short_period_return" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                <field name="awb_special_tax_or_inter_tax_treaty" required="1" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                <field name="awb_specify" attrs="{'invisible': ['|',('awb_special_tax_or_inter_tax_treaty','=','n'), ('awb_state', '!=', 'draft')], 'required':[('awb_special_tax_or_inter_tax_treaty','=','y')]}"/>
                            </group>
                        </group>
                    <notebook >
                        <page name="part_1" string="Computation of Tax">
                            <group>
                                <group>
                                    <field name="awb_15_a" readonly="1"/>
                                    <field name="awb_16_a" readonly="1"/>
                                    <field name="awb_17" readonly="1"/>
                                    <field name="awb_19_a" readonly="1"/>
                                    <field name="awb_20_a" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_20_c" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_20_e" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_21_a" readonly="1"/>
                                    <field name="awb_21_c" readonly="1"/>
                                    <field name="awb_21_e" readonly="1"/>
                                    <field name="awb_21_g" readonly="1"/>
                                    <field name="awb_21_i" readonly="1"/>
                                    <field name="awb_21_k" readonly="1"/>
                                    <field name="awb_21_m" readonly="1"/>
                                    <field name="awb_21_0" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_22" readonly="1"/>
                                    <field name="awb_23_a" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_23_c" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_23_e" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_24" readonly="1"/>
                                    <field name="awb_26_a" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_26_c" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_26_e" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_26_g" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_27" readonly="1"/>
                                    <field name="awb_28_a" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_28_c" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                </group>
                                <group>
                                    <field name="awb_15_b" readonly="1"/>
                                    <field name="awb_16_b" readonly="1"/>
                                    <field name="awb_18" readonly="1"/>
                                    <field name="awb_19_b" readonly="1"/>
                                    <field name="awb_20_b" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_20_d" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_20_f" readonly="1"/>
                                    <field name="awb_21_b" readonly="1"/>
                                    <field name="awb_21_d" readonly="1"/>
                                    <field name="awb_21_f" readonly="1"/>
                                    <field name="awb_21_h" readonly="1"/>
                                    <field name="awb_21_j" readonly="1"/>
                                    <field name="awb_21_l" readonly="1"/>
                                    <field name="awb_21_n" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_21_p" readonly="1"/>
                                    <field name="awb_23_b" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_23_d" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_23_f" readonly="1"/>
                                    <field name="awb_25" readonly="1"/>
                                    <field name="awb_26_b" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_26_d" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_26_f" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_26_h" readonly="1"/>
                                    <field name="awb_28_b" attrs="{'readonly': [('awb_state', '!=', 'draft')]}"/>
                                    <field name="awb_28_d" readonly="1"/>
                                    <field name="awb_29" readonly="1"/>
                                </group>
                            </group>
                        </page>
                        <page name="part_2" string="Accounting" attrs="{'invisible': [('awb_state', '=', 'draft')]}">
                            <group>
                                <group>
                     
                                    <field name="awb_journal_id" attrs="{'required': [('awb_state', '!=', 'draft')],'readonly':[('awb_entry_count', '=',True)]}" options="{'no_create_edit': True,'no_create': True,'no_open': True}"/>
                                    
                                    <field name="awb_move_id" options="{'no_create_edit':True,'no_create': True}" attrs="{'invisible': [('awb_entry_count', '=',False)]}" readonly="1"/>
                                </group>
                            </group>
                        </page>
                    </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>
        <record id="q_2550_tree_view" model="ir.ui.view">
            <field name="name">q.2550</field>
            <field name="model">q.2550</field>
            <field name="arch" type="xml">
                <tree string="2550Q PDF Report">
                    <field name="create_date" widget="date" invisible="1"/>
                    <field name="awb_company_id"/>
                    <field name="awb_fiscal_year"/>
                    <field name="awb_fiscal_quarter" string="Fiscal Quarter"/>
                    <field name="awb_amended_return"/>
                    <field name="awb_no_of_Sheets"/>
                    <field name="awb_special_tax_or_inter_tax_treaty"/>
                    <field name="awb_state" string="Status"/>
                </tree>
            </field>
        </record>
        <record id="action_q_2550_form" model="ir.actions.act_window">
            <field name="name">2550 Q</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">q.2550</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="menu_2550q_reports"
                name="2550Q"
                parent="menu_philippine_tax_reports"
                action="action_q_2550_form"
                />
  </data>
</odoo>