<odoo>
    <data>
        <record id="view_wizard_purchase_relief_reports_excel_report_form" model="ir.ui.view">
            <field name="name">wizard.purchase.relief.reports.excel.form</field>
            <field name="model">wizard.purchase.relief.reports</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Purchase Relief">
                    <group>
                        <field name="awb_company_id" required="1" options="{'no_create_edit': True,'no_create': True,'no_open': True}"/>
                        <field name="awb_fiscal_year" attrs="{'invisible': [('awb_company_id','=', False)]}" options="{'no_create_edit': True,'no_create': True, 'no_open': True}" required="1"/>
                        <field name="awb_fiscal_qua"  options="{'no_create_edit': True,'no_create': True,'no_open': True}" required="1" />
                        <!-- domain="['|', ('awb_type', '=', 'quarterly'),('fiscalyear_id_qua','=', awb_fiscal_year)]" attrs="{'invisible': ['|', '|',('awb_company_id','=', False),('awb_fiscal_year', '=', False)]}" -->
                    </group>

                    <footer>
                        <button name="print_purchase_relief_excel_report" type="object" string='Print Report'   class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary"
                                special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
           <!-- Excel Report Wizard Action -->
        <record model="ir.actions.act_window"
                id="action_wizard_purchase_relief_reports_excel_view">
            <field name="name">Purchase Relief</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard.purchase.relief.reports</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <menuitem id="menu_wizard_purchase_relief_reports_excel"
                  name="Purchase Relief"
                  parent="menu_philippine_tax_reports"
                  action="action_wizard_purchase_relief_reports_excel_view"
                  />
    </data>
</odoo>