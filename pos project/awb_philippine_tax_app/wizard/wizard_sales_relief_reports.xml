<odoo>
    <data>
        <record id="view_wizard_sales_relief_reports_excel_report_form" model="ir.ui.view">
            <field name="name">wizard.sales.relief.reports.excel.form</field>
            <field name="model">wizard.sales.relief.reports</field>
            <field name="type">form</field>
            <!-- <field name="priority" eval="8"/> -->
            <field name="arch" type="xml">
                <form string="Sales Relief">
                    <group>
                        <field name="awb_company_id_sale" required="1" options="{'no_create_edit': True,'no_create': True,'no_open': True}"/>
                        <field name="awb_fiscal_year_sale" attrs="{'invisible': [('awb_company_id_sale','=', False)]}" options="{'no_create_edit': True,'no_create': True,'no_open': True}" required="1"/>
                        <field name="awb_fiscal_qua_sale"  options="{'no_create_edit': True,'no_create': True,'no_open': True}" required="1" />
                        <!-- domain="['|', ('awb_type', '=', 'quarterly'),('fiscalyear_id_qua','=', 'awb_fiscal_year_sale')]" attrs="{'invisible': ['|', '|',('awb_company_id_sale','=', False),('awb_fiscal_year_sale', '=', False)]}" -->
                    </group>
                    <footer>
                        <button name="print_sale_relief_excel_report" type="object" string='Print Report' class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary"
                                special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
           <!-- Excel Report Wizard Action -->
        <record model="ir.actions.act_window"
                id="action_wizard_sales_relief_reports_excel_view">
            <field name="name">Sales Relief</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard.sales.relief.reports</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <menuitem id="menu_philippine_tax_reports"
                name="VAT Reports"
                parent="account.menu_finance_reports"
                groups="awb_philippine_tax_app.report_Philippine_group_1"/>

        <menuitem id="menu_ewt_reports"
                name="EWT Reports"
                parent="account.menu_finance_reports"
                groups="awb_philippine_tax_app.report_Philippine_group_1"/>

        <menuitem id="menu_wizard_sales_relief_reports_excel"
                  name="Sales Relief"
                  action="action_wizard_sales_relief_reports_excel_view"
                  parent="menu_philippine_tax_reports"
                  />
    </data>
</odoo>