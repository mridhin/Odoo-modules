<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="void_model_wizard_view_form" model="ir.ui.view">
		<field name="name">void.popup.wizard.form</field>
		<field name="model">void.popup</field>
		<field name="arch" type="xml">
			<form string="Test">
				<sheet>
				<group class="oe_title">
					<field name="awb_pwd" />
					<field name="awb_void_reason" />
				</group>
				<footer>
                        <button name="CheckPwd" string="Submit" type="object" class="btn-primary" />
                        <button special="cancel" string="Close" class="oe_link" />
                </footer>
                </sheet>
			</form>
		</field>
	</record>
	<record id="void_model_wizard_action" model="ir.actions.act_window">
		<field name="name">Void Orders</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">void.popup</field>
		<field name="view_mode">form</field>
		<field name="view_id" ref="void_model_wizard_view_form" />
		<field name="target">new</field>
	</record>
</odoo>