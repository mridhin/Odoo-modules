    SC/PWD 5%                 <t t-if="x['sc_pwd_vat_in']"> <t t-esc="currency_id_symbol" /> <t t-esc="x['sc_pwd_vat_in']" t-options="{'widget': 'float', 'decimal_precision': 'Product Unit of Measure'}" /></t><t t-else=" "><t t-esc="currency_id_symbol" /> 0.00</t>
    SC 20%                    <t t-if="x['sc_vat_ex']"> <t t-esc="currency_id_symbol" /> <t t-esc="x['sc_vat_ex']" t-options="{'widget': 'float', 'decimal_precision': 'Product Unit of Measure'}" /></t><t t-else=" "><t t-esc="currency_id_symbol" /> 0.00</t>
    PWD 20%                   <t t-if="x['pwd_vat_ex']"> <t t-esc="currency_id_symbol" /> <t t-esc="x['pwd_vat_ex']" t-options="{'widget': 'float', 'decimal_precision': 'Product Unit of Measure'}" /></t><t t-else=" "><t t-esc="currency_id_symbol" /> 0.00</t>
    
    
                  POS Provider
              <t t-if='env.company.awb_pos_provider_id'><t t-esc="env.company.awb_pos_provider_id.name" /></t>
