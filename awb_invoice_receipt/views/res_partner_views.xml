<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="awb_view_partner_form" model="ir.ui.view">
        <field name="name">awb.res.partner.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="before">
                <field name="awb_customer_tin" invisible="1"/>
                <field name="awb_invoice_receipt_business_style" />
            </xpath>
        </field>
    </record>

</odoo>
