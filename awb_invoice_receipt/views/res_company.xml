<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="awb_view_company_form" model="ir.ui.view">
            <field name="name">awb.res.company.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="awb_invoice_receipt_business_style" />
                    <field name="acknowledgement_certificate"/>
                    <field name="certificate_date_issued"/>
                    <field name="series_range"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
