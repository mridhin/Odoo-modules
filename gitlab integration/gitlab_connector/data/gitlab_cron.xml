<odoo>
    <record id="ir_cron_import_gitlab_project" model="ir.cron">
        <field name="name">Gitlab : Import Project</field>
        <field name="model_id" ref="model_gitlab_connector"/>
        <field name="state">code</field>
        <field name="code">model.cron_import_gitlab_data()</field>
        <field name="interval_number">15</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</odoo>
