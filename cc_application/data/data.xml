        </record>
        
        <record id="section_permissions" model="website.section">
            <field name="name">Permissions and Consent</field>
        </record>
        
        <record id="section_deliverability" model="website.section">
            <field name="name">Deliverability</field>
        </record>
        
        <record id="section_risks" model="website.section">
            <field name="name">Risks</field>
        </record>
        
        <record id="section_timescales" model="website.section">
