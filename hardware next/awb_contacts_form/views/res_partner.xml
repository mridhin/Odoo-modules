<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <!-- Inherited and added new fields for contacts table -->
        <record id="inherit_view_partner_form" model="ir.ui.view">
	        <field name="name">res.partner.form.inherit</field>
	        <field name="model">res.partner</field>
	        <field name="inherit_id" ref="base.view_partner_form" />
	        <field name="priority" eval="18" />
	        <field name="arch" type="xml">
	            <field name="vat" position="after">
	               <field name="ship_to_id"/>
	               <field name="delivery_notes"/>
		           <field name="collection_address"/>
		           <field name="collection_notes"/>
	               <field name="incentive_eligible"/>
		           <field name="agents_id"/>
		           <field name="customer_type"/>
		           <field name="invoice_type"/>
		           <field name="delivery_area_id"/>
		           <field name="collection_area"/>
		        </field>
		       <field name="website" position="after">
	         		<field name="where_did_you_find_out_about_us"/>
	           </field>
	           <field name="category_id" position="after">
	         		<field name="vendor_type"/>
	           </field>
	        </field>
        </record>
    </data>
 </odoo>