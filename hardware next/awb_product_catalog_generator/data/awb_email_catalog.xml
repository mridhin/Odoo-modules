<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<record id="email_template_edi_product_catalog" model="mail.template">
			<field name="name">Product Catalog: Send by email</field>
			<field name="subject">${'Product Catalog'} (Ref
				${object.name or 'n/a' })</field>
			<field name="model_id"
				ref="awb_product_catalog_generator.model_product_catalog" />
			<field name="auto_delete" eval="False" />
			<field name="report_name">${(object.name or '').replace('/','-')}</field>
			<field name="body_html">
				<![CDATA[
					<p>Dear Sir</p>
					<p>Here is Product Catalog </p>
					<p>Thank you,</p>
					<p style="color:#888888;">
					% if object.user_id and object.user_id.signature:
						${object.user_id.signature | safe}
					% endif
					</p>
				]]>
			</field>
		</record>
	</data>
</odoo>